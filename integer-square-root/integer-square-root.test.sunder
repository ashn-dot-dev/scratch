import "std";
import "integer-square-root.sunder";

func main() void {
    assert isqrt[[usize]](0) == 0;
    assert isqrt[[usize]](1) == 1;
    assert isqrt[[usize]](2) == 1;
    assert isqrt[[usize]](3) == 1;
    assert isqrt[[usize]](4) == 2;
    assert isqrt[[usize]](5) == 2;
    assert isqrt[[usize]](6) == 2;
    assert isqrt[[usize]](7) == 2;
    assert isqrt[[usize]](8) == 2;
    assert isqrt[[usize]](9) == 3;
    assert isqrt[[usize]](10) == 3;
    assert isqrt[[usize]](100) == 10;
    assert isqrt[[usize]](101) == 10;

    assert isqrt[[ssize]](0) == 0;
    assert isqrt[[ssize]](1) == 1;
    assert isqrt[[ssize]](2) == 1;
    assert isqrt[[ssize]](3) == 1;
    assert isqrt[[ssize]](4) == 2;
    assert isqrt[[ssize]](5) == 2;
    assert isqrt[[usize]](6) == 2;
    assert isqrt[[usize]](7) == 2;
    assert isqrt[[usize]](8) == 2;
    assert isqrt[[usize]](9) == 3;
    assert isqrt[[usize]](10) == 3;
    assert isqrt[[ssize]](100) == 10;
    assert isqrt[[ssize]](101) == 10;
}
